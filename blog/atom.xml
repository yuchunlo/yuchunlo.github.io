---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Yu-Chun Lo</title>
	<link href="http://yuchunlo.tk/blog/atom.xml" rel="self"/>
	<link href="http://yuchunlo.tk/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://yuchunlo.tk/blog</id>
	<author>
		<name>Yu-Chun Lo</name>
		<email>yuchunlo.2401@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://yuchunlo.tk{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://yuchunlo.tk/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
